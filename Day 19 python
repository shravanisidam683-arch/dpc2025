import math

def count_divisors(n):
    count = 0
    for i in range(1, int(math.sqrt(n)) + 1):
        if n % i == 0:
            count += 1  # i is a divisor
            if i != n // i:
                count += 1  # n // i is also a divisor
    return count

N = 12
print("Number of divisors:", count_divisors(N))  # Output: 6
